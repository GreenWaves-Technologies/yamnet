set log_level INFO
adjust 
fusions --scale8 
dsp_preprocessing input_1 MFCCPreprocessingParameters --config_json models/logmel_config.json
set input_norm_func "x: x/(1<<15)"
aquant --scheme SQ8 calibration_features/speech_whistling_cut.wav --hwc --slices 0 15600 1
adjust
qtune --step MfccPreprocessing scheme=float float_type=float32
qtune --step input_1 scheme=pow2
fusions --scale8

# RAM/FLASH Settings
set l3_ram_device $(MODEL_L3_RAM)
set l3_flash_device $(MODEL_L3_FLASH)
set graph_const_exec_from_flash true

nodeoption input_1 ALLOCATE 1
#set graph_dump_tensor 4
#set graph_dump_one_node S64_Op_MfccPreprocessing
set graph_produce_node_names true
set graph_produce_operinfos true
set graph_monitor_cycles true
save_state
